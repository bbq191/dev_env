{
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "bazel.buildifierFixOnFormat": true,
  "debug.showBreakpointsInOverviewRuler": true,
  "editor.acceptSuggestionOnEnter": "smart",
  "editor.accessibilitySupport": "off",
  "editor.codeActionsOnSave": {
    "source.addMissingImports": "always",
    "source.organizeImports": "always"
  },
  "editor.cursorBlinking": "solid",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorStyle": "block",
  "editor.cursorSurroundingLines": 8,
  "editor.dragAndDrop": false,
  "editor.fontFamily": "'Comic Code Ligatures','Source Han Sans SC','Symbols Nerd Font Mono'",
  "editor.fontLigatures": "'calt','zero','ss01'", // 参考 https://github.com/JetBrains/JetBrainsMono/wiki/OpenType-features
  "editor.formatOnSave": true,
  "editor.inlineSuggest.enabled": true,
  "editor.lineNumbers": "relative",
  "editor.linkedEditing": true,
  "editor.minimap.enabled": false,
  "editor.rulers": [
    {
      "color": "#00FF0010",
      "column": 80
    },
    {
      "color": "#BDB76B15",
      "column": 100
    },
    {
      "color": "#FA807219",
      "column": 120
    }
  ],
  "editor.scrollBeyondLastLine": false,
  "editor.smoothScrolling": true,
  "editor.stickyScroll.enabled": true,
  "editor.suggest.insertMode": "replace",
  "editor.suggest.snippetsPreventQuickSuggestions": false,
  "editor.suggestFontSize": 13,
  "editor.fontSize": 13,
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "editor.unicodeHighlight.includeComments": true,
  "editor.wordWrap": "on",
  "emmet.variables": {
    "lang": "en"
  },
  "errorLens.fontStyleItalic": true,
  "errorLens.fontWeight": "300",
  "errorLens.gutterEmoji": {
    "error": "E",
    "hint": "H",
    "info": "I",
    "warning": "W"
  },
  "errorLens.gutterIconSet": "emoji",
  "errorLens.gutterIconSize": "80%",
  "errorLens.gutterIconsEnabled": true,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "extensions.autoUpdate": "onlyEnabledExtensions",
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "extensions.ignoreRecommendations": false,
  "files.exclude": {
    "**/node_modules": true
  },
  "git.autofetch": true,
  "git.path": "/opt/homebrew/bin/git",
  "java.compile.nullAnalysis.mode": "automatic",
  "java.configuration.detectJdksAtStart": false,
  "java.configuration.maven.userSettings": "${env:XDG_CONFIG_HOME}/maven/settings.xml",
  "java.configuration.runtimes": [
    {
      "name": "JavaSE-1.8",
      "default": true,
      "path": "/Users/afu/.local/share/sdkman/candidates/java/8.0.392-amzn"
    },
    {
      "name": "JavaSE-17",
      "path": "/Users/afu/.local/share/sdkman/candidates/java/17.0.10-jbr"
    },
    {
      "name": "JavaSE-21",
      "path": "/Users/afu/.local/share/sdkman/candidates/java/21.0.5-tem"
    }
  ],
  "window.zoomLevel": -1,
  "java.configuration.updateBuildConfiguration": "automatic",
  "java.debug.settings.hotCodeReplace": "auto",
  "java.dependency.packagePresentation": "hierarchical",
  "java.maxConcurrentBuilds": 12,
  "java.sources.organizeImports.staticStarThreshold": 1,
  "maven.executable.path": "$home/.local/share/sdkman/candidates/maven/current/bin/mvn",
  "maven.terminal.customEnv": [
    {
      "environmentVariable": "JAVA_HOME",
      "value": "$home/.local/share/sdkman/candidates/java/8.0.392-amzn"
    }
  ],
  "prettier.semi": false,
  "prettier.singleAttributePerLine": true,
  "prettier.singleQuote": true,
  "prettier.trailingComma": "all",
  "projectManager.git.baseFolders": ["$home/Developer"],
  "projectManager.sortList": "Recent",
  "python.autoComplete.extraPaths": [
    "${workspaceFolder}/venv/lib/python3.13/site-packages"
  ],
  "python.defaultInterpreterPath": "${workspaceFolder}/venv/bin/python",
  "python.envFile": "${userHome}/Developer/py-projects",
  "python.terminal.activateEnvironment": true,
  "python.venvPath": "${workspaceFolder}/venv",
  "redhat.telemetry.enabled": false,
  "rust-analyzer.check.command": "clippy",
  "sortJSON.orderOverride": [
    "name",
    "version",
    "main",
    "module",
    "types",
    "typings",
    "files",
    "publishConfig",
    "repository",
    "scripts",
    "prefix",
    "description",
    "body"
  ],
  "sortJSON.orderUnderride": [
    "resolutions",
    "dependencies",
    "devDependencies",
    "peerDependencies",
    "cSpell.userWords"
  ],
  "spring-boot.ls.java.vmargs": [
    "-Xmx8G",
    "-Dlanguageserver.boot.symbolCacheDir=$home/cache/sts4/symbolCache"
  ],
  "terminal.external.osxExec": "kitty.app",
  "terminal.integrated.defaultProfile.osx": "zsh",
  "terminal.integrated.enablePersistentSessions": false,
  "terminal.integrated.profiles.osx": {
    "JavaSE-1.8 LTS": {
      "env": {
        "JAVA_HOME": "/Users/afu/.local/share/sdkman/candidates/java/8.0.392-amzn"
      },
      "overrideName": true,
      "path": "zsh"
    },
    "JavaSE-17 LTS": {
      "env": {
        "JAVA_HOME": "/Users/afu/.local/share/sdkman/candidates/java/17.0.10-jbr"
      },
      "overrideName": true,
      "path": "zsh"
    },
    "JavaSE-21 LTS": {
      "env": {
        "JAVA_HOME": "/Users/afu/.local/share/sdkman/candidates/java/21.0.5-tem"
      },
      "overrideName": true,
      "path": "zsh"
    },
    "zsh": {
      "env": {
        "JAVA_HOME": "/Users/afu/.local/share/sdkman/candidates/java/21.0.5-tem"
      },
      "path": "zsh"
    }
  },
  "terminal.integrated.tabs.hideCondition": "never",
  "typescript.preferences.importModuleSpecifier": "relative",
  "update.showReleaseNotes": false,
  "vim.foldfix": true,
  "vim.highlightedyank.color": "rgba(230, 97, 89, 0.7)",
  "vim.highlightedyank.enable": true,
  "vim.highlightedyank.textColor": "white",
  "vim.hlsearch": true,
  "vim.insertModeKeyBindingsNonRecursive": [
    {
      "before": ["j", "k"],
      "commands": ["workbench.action.files.save", "extension.vim_escape"]
    },
    {
      "before": ["<c-x>"],
      "commands": ["editor.action.clipboardCutAction"]
    }
  ],
  "vim.leader": "<space>",
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["leader", "l", "g"],
      "commands": ["workbench.view.scm"]
    },
    {
      "before": ["leader", "p", "v"],
      "commands": ["workbench.view.explorer"]
    },
    {
      "before": ["leader", "c"],
      "commands": ["workbench.panel.chat.view.copilot.focus"]
    },
    {
      "before": ["leader", "f", "s"],
      "commands": [
        "cursorWordStartLeft",
        "cursorWordEndRightSelect",
        "workbench.action.findInFiles"
      ]
    },
    {
      "before": ["leader", "s"],
      "commands": [
        "cursorWordStartLeft",
        "cursorWordEndRightSelect",
        "editor.action.startFindReplaceAction"
      ]
    },
    {
      "before": ["C-j"],
      "commands": ["editor.action.marker.prev"]
    },
    {
      "before": ["C-k"],
      "commands": ["editor.action.marker.prev"]
    },
    {
      "before": ["leader", "k"],
      "commands": ["editor.action.marker.nextInFiles"]
    },
    {
      "before": ["leader", "j"],
      "commands": ["editor.action.marker.prevInFiles"]
    },
    {
      "before": ["leader", "r"],
      "commands": ["editor.action.rename"]
    },
    {
      "before": ["leader", "h"],
      "commands": ["editor.action.showHover"]
    },
    {
      "after": ["\"", "_", "x"],
      "before": ["x"]
    },
    {
      "after": ["\"", "+", "y", "$"],
      "before": ["leader", "s-y"]
    },
    {
      "after": ["\"", "_", "d"],
      "before": ["leader", "d"]
    },
    {
      "before": ["leader", "w"],
      "commands": ["workbench.action.files.saveAll"]
    },
    {
      "before": ["leader", "f"],
      "commands": ["editor.action.formatDocument"]
    }
  ],
  "vim.statusBarColorControl": true,
  "vim.statusBarColors.commandlineinprogress": "#007ACC",
  "vim.statusBarColors.easymotioninputmode": "#007ACC",
  "vim.statusBarColors.easymotionmode": "#007ACC",
  "vim.statusBarColors.insert": "#BF616A",
  "vim.statusBarColors.normal": ["#8FBCBB", "#434C5E"],
  "vim.statusBarColors.replace": "#D08770",
  "vim.statusBarColors.searchinprogressmode": "#007ACC",
  "vim.statusBarColors.surroundinputmode": "#007ACC",
  "vim.statusBarColors.visual": "#B48EAD",
  "vim.statusBarColors.visualblock": "#A3BE8C",
  "vim.statusBarColors.visualline": "#B48EAD",
  "vim.surround": true,
  "vim.useCtrlKeys": true,
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "after": ["\"", "_", "d", "P"],
      "before": ["leader", "p"]
    },
    {
      "after": ["\"", "+", "y"],
      "before": ["leader", "y"]
    },
    {
      "after": ["\"", "_", "d"],
      "before": ["leader", "d"]
    }
  ],
  "window.commandCenter": false,
  "window.dialogStyle": "custom",
  "workbench.activityBar.location": "top",
  "workbench.colorCustomizations": {
    "[Default Dark Modern]": {
      "tab.activeBorderTop": "#00FF00",
      "tab.unfocusedActiveBorderTop": "#00FF0088",
      "textCodeBlock.background": "#00000055"
    },
    "editor.selectionHighlightBorder": "#A9A9A9",
    "editor.wordHighlightBorder": "#FFD700",
    "editor.wordHighlightStrongBorder": "#FF6347",
    "statusBar.background": "#8FBCBB",
    "statusBar.noFolderBackground": "#8FBCBB",
    "statusBar.debuggingBackground": "#8FBCBB",
    "statusBar.foreground": "#434C5E",
    "statusBar.debuggingForeground": "#434C5E"
  },
  "workbench.colorTheme": "Visual Studio Light",
  "workbench.editor.revealIfOpen": true,
  "workbench.editor.showTabs": "multiple",
  "workbench.list.smoothScrolling": true,
  "workbench.panel.defaultLocation": "left",
  "workbench.settings.editor": "json",
  "workbench.sideBar.location": "right",
  "workbench.startupEditor": "readme",
  "workbench.tree.indent": 10,
  "zenMode.hideLineNumbers": false
}
